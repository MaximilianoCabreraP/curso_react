turn () => {\r\n                cargando = false;\r\n            }\r\n        }\r\n    },[idOrden])\r\n\r\n    const vaciarPedidos = () => {\r\n        setIdOrden([]);\r\n        setListaOrden([]);\r\n    }\r\n\r\n    let curDate = new Date(null);\r\n    \r\n    if(loading){\r\n        return \"Cargando...\";\r\n    }else{\r\n        return (\r\n            <div>\r\n                {listaOrden.length ?\r\n                    <div className=\"container\">\r\n                        {/* <h2>Lista de pedidos</h2>\r\n                        <div className=\"container my-1 alert alert-dismissible alert-success\">\r\n                            La compra fue exitosa. Tu nro de pedido es: <h5>{idOrden}</h5>\r\n                        </div> */}\r\n\r\n                        <button onClick={vaciarPedidos} type=\"button\" className=\"btn btn-primary mb-2 vaciar-pedidos\">Vaciar Pedidos</button> \r\n                        <table className=\"table table-hover\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th scope=\"col\">NroPedido</th>\r\n                                    <th scope=\"col\">Fecha</th>\r\n                                    <th scope=\"col\">Producto</th>\r\n                                    <th scope=\"col\">Precio Unitario</th>\r\n                                    <th scope=\"col\">Total</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                {listaOrden.map(({id, date, items, total}, i) => (\r\n                                    <tr className={i===0?\"table-success\":\"\"}>\r\n                                        <th scope=\"row\"><p >{id}</p></th>\r\n                                        <td className=\"text-truncate\">{\r\n                                            curDate.setTime(date.seconds*1000),\r\n                                            curDate.toLocaleString()\r\n                                            }</td>\r\n                                        <td>{items && items.map(({cantidad, item}) => <p className=\"acomodo-linea text-truncate\">{cantidad} x {item.title}</p>)}</td>\r\n                                        <td>{items &&\r\n                                            items.map(({item}) => <p className=\"acomodo-linea\">${item.price}</p>)}</td>\r\n                                        <td>${total}</td>\r\n                                    </tr>\r\n                                ))\r\n                            }\r\n                            </tbody>\r\n                        </table>\r\n                    </div> \r\n                :\r\n                    <>\r\n                        <div className=\"p-3 text-center text-muted\">No realizaste ningún pedido</div>\r\n                        <Link to=\"/\" className=\"btn btn-primary\">Empezar a comprar</Link>\r\n                    </>\r\n                }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default MisPedidos\r\n","C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\components\\Header\\Logo.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\components\\Header\\NavBar.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\components\\Body\\Loader.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\context\\CartContext.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\components\\Body\\ItemCarrito.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\firebaseConfig.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\components\\Body\\ItemList.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\components\\Body\\ItemDetail.js",[],"C:\\Users\\MCabrera\\Documents\\Meme\\Curso React\\curso_react\\src\\Assets\\Products.js",[],"C:\\Us